  "keywords": ["salesforce", "rest", "api", "sdk"],
  "license": "MIT",
  "authors": [
    {
      "name": "Arthur Guy",
      "email": "arthur.guy@crunch.co.uk"
    }
  ],
  "require": {
    "nesbot/carbon": "^1.19",
    "guzzlehttp/guzzle": "^6.0"
  },
  "require-dev": {
